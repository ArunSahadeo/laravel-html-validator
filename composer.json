{
    "name": "aruns/laravel-html-validator",
    "description": "Provides a Laravel artisan command for validating your Laravel install's HTML views.",
    "keywords": ["laravel", "composer", "artisan"],
    "license": "GPLv3+",
    "authors": [
        {
            "name": "Arun James Sahadeo",
            "email": "arunjamessahadeo@gmail.com"
        }
    ],
    "type": "laravel-plugin",
    "require": {
		"php": ">=5.4"
    },
	"require-dev": {
		"phpunit/phpunit": "5.2.*"
    },
	"autoload": {
        "psr-4": {
            "Aruns\\LaravelHTMLValidator\\": "src/"
        }
    },
	"extra": {
		"laravel": {
			"providers": [
				"Aruns\\LaravelHTMLValidator\\HTMLValidator"
			]
		}
	}
}
